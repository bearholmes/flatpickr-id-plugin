{
  "name": "flatpickr-id-plugin",
  "version": "1.0.3",
  "description": "A flatpickr plugin for transferring ID and accessibility attributes to the generated input element",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts src",
    "prepare": "husky install",
    "prepublishOnly": "npm run build:prod && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bearholmes/flatpickr-id-plugin.git"
  },
  "keywords": [
    "flatpickr",
    "plugin",
    "id",
    "accessibility",
    "aria",
    "datepicker"
  ],
  "author": "bearholmes <bearholmes@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bearholmes/flatpickr-id-plugin/issues"
  },
  "homepage": "https://github.com/bearholmes/flatpickr-id-plugin#readme",
  "peerDependencies": {
    "flatpickr": "^4.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "rollup": "^4.6.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "flatpickr": "^4.6.0"
  }
}
